#include<stdio.h>
int main(){
	
	int ch=0;
	printf("\n 1.PIZZA : $399 \n 2. BURGER : $139 \n 3.PASTA : $179 \n4.french fries: $89 \n 5.Sandwich : $249 ");
	printf("\nEnter your choice  :");
	scanf("%d",&ch);
	
	switch(ch){
		case 1 : printf("\n 1.PIZZA : $399");break;
		case 2 : printf("\n 2. BURGER : $139");break;
		case 3 : printf("\n 3.PASTA : $179"); break;
		case 4 : printf("\n 4.french fries: $89");break;
		case 5 : printf("\n 5.Sandwich : $249");break;
		default : printf("\n default case not prefect choice");
		
	}
	
	
	
	
